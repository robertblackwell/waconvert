{
    "name":"robertblackwell/waconvert",
    "description": "Database conversion utility for whiteacorn website",
    "keywords": ["trace", "php", "debug"],
    "type": "library",
    "bin" : ["bin/waconvert.php"],
    "homepage": "https://github.com/robertblackwell/waconvert",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Blackwell",
            "homepage": "https://github.com/robertblackwell"
        }
    ],
    
    "repositories": [
        {"type": "vcs","url": "/Users/robertblackwell/git-repos/database-package"},
        {"type": "vcs","url": "/Users/robertblackwell/git-repos/gps"},
